{
  "name": "vulcanval",
  "version": "3.0.1",
  "description": "Validate data forms in client and server sides seamlessly",
  "keywords": [
    "validation",
    "form",
    "forms",
    "jquery",
    "plugin"
  ],
  "private": false,
  "author": "Romel PÃ©rez (romelperez.com)",
  "license": "MIT",
  "homepage": "https://github.com/vulcan-estudios/vulcanval",
  "repository": {
    "type": "git",
    "url": "https://github.com/vulcan-estudios/vulcanval.git"
  },
  "bugs": {
    "url": "https://github.com/vulcan-estudios/vulcanval/issues"
  },
  "main": "bundle/js/vulcanval.js",
  "scripts": {
    "all": "npm run build && npm run bundle && npm run doc && npm run test",
    "build": "./node_modules/.bin/gulp build",
    "bundle": "./node_modules/.bin/babel src --out-dir bundle",
    "test": "npm run test-global && npm run test-jquery",
    "test-global": "npm run test-global-node && npm run test-global-karma",
    "test-global-node": "NODE_ENV=test ./node_modules/.bin/mocha test/global",
    "test-global-karma": "NODE_ENV=test ./node_modules/.bin/karma start karma-global.conf.js",
    "test-jquery": "NODE_ENV=test ./node_modules/.bin/karma start karma-jquery.conf.js",
    "test-jquery:desktop": "NODE_ENV=test ./node_modules/.bin/karma start karma-desktop.conf.js",
    "docs": "./node_modules/.bin/gulp docs"
  },
  "peerDependencies": {
    "validator": "5.x",
    "jquery": "*"
  },
  "dependencies": {
    "extend": "^3.0.0",
    "mock-require": "^1.3.0",
    "prhone-log": "^2.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "del": "^2.2.1",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.7",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "ink-docstrap": "^1.2.1",
    "karma": "^1.1.0",
    "karma-browserify": "^5.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.0.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "require-dir": "^0.3.0",
    "sassdoc": "^2.1.20",
    "validator": "5.x",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
